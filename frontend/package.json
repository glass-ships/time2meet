{
  "name": "whenworks",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "compile": "npx ts-node --transpileOnly --esm --experimental-specifier-resolution=node --files ./compile/index.ts",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix && prettier --write \"**/*.{tsx,ts,css}\" --no-error-on-unmatched-pattern",
    "test:lint": "eslint . && prettier \"**/*.{tsx,ts,css}\" --no-error-on-unmatched-pattern"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-router": "^7.0.1",
    "react-dom": "^18.3.1",
    "sass": "^1.81.0"
  },
  "devDependencies": {
    "@types/node": "^22.9.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-plugin-svgr": "^4.3.0"
  },
  "trustedDependencies": [
    "@parcel/watcher"
  ]
}
